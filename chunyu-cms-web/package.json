{
  "name": "chunyu-cms",
  "private": false,
  "scripts": {
    "build": "nuxt build",
    "build:pro": "nuxt build --dotenv .env.pro",
    "dev:pro": "nuxt dev --host 0.0.0.0 --dotenv .env.pro",
    "dev": "nuxt dev --host 0.0.0.0",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "pnpm build && pm2 restart chunyu-cms-web",
    "lint:js": "eslint --ext \".ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "pnpm lint:js && pnpm lint:prettier",
    "lintfix": "prettier --write --list-different . && pnpm lint:js --fix",
    "generate": "drizzle-kit generate",
    "sql-push": "drizzle-kit push",
    "deploy-update": "pm2 delete chunyu-cms-web && docker stop mysql8 && docker stop redis && pnpm build && docker start mysql8 && docker start redis && pm2 start pm2.config.cjs"
  },
  "devDependencies": {
    "@element-plus/nuxt": "^1.1.1",
    "@iconify-json/el": "^1.2.1",
    "@iconify-json/flat-color-icons": "^1.2.1",
