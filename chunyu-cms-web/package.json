    "lodash": "^4.17.21",
    "mysql2": "^3.14.1",
    "nanoid": "^5.1.5",
    "node-schedule": "^2.1.1",
    "node-xlsx": "^0.24.0",
    "nodemailer": "^6.10.1",
    "qrcode.vue": "^3.6.0",
    "sass": "^1.87.0",
    "showdown": "^2.1.0",
    "svg-captcha": "^1.4.0",
    "swiper": "^11.2.6",
    "systeminformation": "^5.25.11",
    "ua-parser-js": "^2.0.3",
